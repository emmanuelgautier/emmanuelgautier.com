{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "next",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "English Posts",
      "path": "[[workspace]]/content/en/_posts"
    },
    {
      "title": "English Snippets",
      "path": "[[workspace]]/content/en/_snippets"
    },
    {
      "title": "English Pages",
      "path": "[[workspace]]/content/en/_pages"
    },
    {
      "title": "French Posts",
      "path": "[[workspace]]/content/fr/_posts"
    },
    {
      "title": "French Snippets",
      "path": "[[workspace]]/content/fr/_snippets"
    },
    {
      "title": "French Pages",
      "path": "[[workspace]]/content/fr/_pages"
    }
  ],
  "frontMatter.content.publicFolder": "out",
  "frontMatter.site.baseURL": "https://www.emmanuelgautier.com",
  "frontMatter.taxonomy.isPublishedDate": "created",
  "frontMatter.taxonomy.isModifiedDate": "updated"
}
