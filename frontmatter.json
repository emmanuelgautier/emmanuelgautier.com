{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "next",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Snippets",
      "path": "content/_snippets",
      "excludeSubdir": true
    },
    {
      "title": "French Pages",
      "path": "content/fr/_pages",
      "excludeSubdir": true
    },
    {
      "title": "English Pages",
      "path": "[[workspace]]/content/en/_pages",
      "excludeSubdir": true
    },
    {
      "title": "French Blog posts",
      "path": "content/fr/_posts"
    },
    {
      "title": "English Blog posts",
      "path": "content/en/_posts"
    }
  ],
  "frontMatter.content.publicFolder": "out",
  "frontMatter.site.baseURL": "https://www.emmanuelgautier.com",
  "frontMatter.taxonomy.dateField": "created",
  "frontMatter.taxonomy.modifiedField": "updated",
  "frontMatter.taxonomy.tags": [
    "aws",
    "docker"
  ]
}
